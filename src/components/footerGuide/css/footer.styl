@import "../../../reset/css/mixin.styl"
.footer_wrapper
  background-color white
  position fixed
  bottom 0
  left 0
  width 100%
  height (120/$rem)
  display flex
  align-items center
  .tab_wrapper
    flex 1
    display flex
    align-items center
    flex-direction column
    &.on
      span
        color #f00
    span.tab_font
      font-size (24/$rem)
    span
      .iconfont
        font-size (50/$rem)
    .icon
      margin (10/$rem) 0
